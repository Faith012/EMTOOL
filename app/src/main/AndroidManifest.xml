<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.emtool">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EMTOOL">
        <activity android:name=".PrismVolumeActivity" />
        <activity android:name=".PyramidVolumeActivity" />
        <activity android:name=".CuboidVolumeActivity" />
        <activity android:name=".CubeVolumeActivity" />
        <activity
            android:name=".InchesCmActivity"
            android:label="@string/inches_cm"
            android:parentActivityName=".SecondActivity" />
        <activity
            android:name=".CelsiusFahrenheitActivity"
            android:label="@string/celsius_fahrenheit"
            android:parentActivityName=".SecondActivity" />
        <activity
            android:name=".RadianDegreeActivity"
            android:label="@string/RD"
            android:parentActivityName=".SecondActivity" />
        <activity
            android:name=".MilesKmActivity"
            android:label="@string/miles_km"
            android:parentActivityName=".SecondActivity" />
        <activity
            android:name=".SecondActivity"
            android:label="@string/ERC"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".FifthActivity"
            android:label="@string/volume_calculator"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".FourthActivity"
            android:label="@string/impedance_calculator"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ThirdActivity"
            android:label="@string/conversions"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>